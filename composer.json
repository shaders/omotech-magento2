{
    "name": "omotech/magento2-integration",
    "description": "Omotech extension for Magento 2.3 and 2.4",
    "type": "magento2-component",
    "version": "1.0.0",
    "license": "OSL-3.0",
    "require": {
        "php": "~7.3.0||~7.4.0||~8.0||~8.1||~8.2||~8.3"
    },
    "replace": {
        "omotech/core": "self.version",
        "omotech/customer": "self.version",
        "omotech/abandonedcart": "self.version",
        "omotech/order": "self.version",
        "omotech/web": "self.version"
    },
    "autoload": {
        "files": [
            "Core/registration.php",
            "Customer/registration.php",
            "AbandonedCart/registration.php",
            "Order/registration.php",
            "Web/registration.php"
        ],
        "psr-4": {
            "Omotech\\Core\\": "Core",
            "Omotech\\Customer\\": "Customer",
            "Omotech\\AbandonedCart\\": "AbandonedCart",
            "Omotech\\Order\\": "Order",
            "Omotech\\Web\\": "Web"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
